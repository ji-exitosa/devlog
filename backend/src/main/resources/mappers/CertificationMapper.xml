<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.devlog.mapper.CertificationMapper">

	<!-- 사용자의 모든 기술스택을 반환한다. -->
	<select id="selectAllCertification" resultType="Certification">
		select *
		from
		certification
	</select>
	
	<insert id="insertCertification">
		insert into certification( `key`,`value` )
		values
		( #{key},#{value} )
	</insert>
	
</mapper>






